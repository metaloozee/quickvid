{
  "name": "quickvid",
  "version": "2.0.0",
  "private": false,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "typecheck": "tsc --noEmit",
    "stripe:login": "stripe login",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/webhooks",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "docker:build": "docker build -t quickvid .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.local quickvid",
    "supabase:fetch-types": "supabase gen types typescript --project-id \"nzgyvjxjamuuuhikvxpl\" --schema public > types/supabase.ts",
    "prepare": "husky && husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,css,md}": "prettier --write",
    "*.js": "eslint --cache --fix"
  },
