{
  "name": "quickvid",
  "version": "2.0.0",
  "private": false,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "docker:build": "docker build -t quickvid .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.local quickvid",
    "prepare": "husky && husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,css,md}": "prettier --write",
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.0.1",
    "@hookform/resolvers": "^3.3.4",
    "@langchain/community": "^0.0.53",
    "@langchain/core": "0.1.58",
    "@langchain/groq": "^0.0.8",
    "@langchain/openai": "^0.0.28",
    "@neondatabase/serverless": "^0.9.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "ai": "^3.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^1.2.1",
    "drizzle-orm": "^0.30.9",
    "form-data": "^4.0.0",
    "geist": "^1.3.0",
    "langchain": "^0.1.36",
    "lucide-react": "0.364.0",
    "next": "14.1.4",
    "next-auth": "5.0.0-beta.17",
    "next-themes": "^0.2.1",
    "next-view-transitions": "^0.1.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.38.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.3",
    "sharp": "^0.33.3",
    "sonner": "^1.4.41",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "ytdl-core": "^4.11.5",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@types/node": "^17.0.45",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "git-format-staged": "^3.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "pg": "^8.11.5",
    "postcss": "^8.4.38",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "youtube-transcript": "^1.2.1",
    "youtubei.js": "^9.3.0"
  },
  "pnpm": {
    "overrides": {
      "@langchain/core": "0.1.60"
    }
  }
}
